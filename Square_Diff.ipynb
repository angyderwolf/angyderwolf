        "def Square_Diff(n):\n",
        "  numerosnat = (n * (n + 1) * (2 * n + 1)) / 6 \n",
        "  cuadrasuma = (n * (n + 1)) / 2\n",
        "  cuadrasuma = cuadrasuma ** 2\n",
        "  diferencia = abs(numerosnat - cuadrasuma)\n",
        "  return int(diferencia)\n",
        " \n",
        "# Driver code\n",
        "print(Square_Diff(10))\n"
      ]
    }
  ]
