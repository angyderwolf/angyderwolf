        "def estadisticas(Inventario):\n",
        "    Cantidad = 0\n",
        "    Promedio = 0\n",
        "    for i in range(len(Inventario)):\n",
        "        Cantidad += Inventario[i][2]\n",
        "        Promedio += Inventario[i][3]\n",
        "    \n",
        "    print(Cantidad, Promedio/len(Inventario))"
      ]
