        "def cambiaalopuesto(palabra):\n",
        "    palabra2 = \"\"\n",
        "    for x in range(len(palabra)):\n",
        "        if palabra[x].isupper():\n",
        "            palabra2 += palabra[x].lower()\n",
        "        else:\n",
        "            palabra2 += palabra[x].upper()\n",
        "\n",
        "    print(palabra2)\n"
      ]
    }
  ]
}