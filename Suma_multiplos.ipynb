        "def suma_multiplos():\n",
        "  cuenta = 0\n",
        "  suma = 0\n",
        "  numero = 0\n",
        "  while cuenta < 1000:\n",
        "    if numero % 3 == 0 or numero % 5 == 0:\n",
        "      suma = suma + numero\n",
        "    numero += 1\n",
        "    cuenta += 1\n",
        "  print(suma)\n",
        "\n",
        "def main():\n",
        "  suma_multiplos()\n",
        "main()\n",
        "\n"
      ]
    }
