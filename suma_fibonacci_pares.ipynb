        "def suma_fibonacci_pares():\n",
        "  suma = 0\n",
        "  x, y = 0, 1\n",
        "  while y < 4000000:\n",
        "    if y % 2 == 0:\n",
        "      suma += y\n",
        "    x,y = y, x+y\n",
        "  print(suma)\n",
        "def main():\n",
        "  suma_fibonacci_pares()\n",
        "main()\n",
        "\n"
      ]
    }
