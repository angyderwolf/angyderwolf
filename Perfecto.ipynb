        "def Perfecto(num):\n",
        "  #Cuerpo de la funcion Perfecto\n",
        "  suma = 0\n",
        "  if num >= 0:\n",
        "    for i in range(1, num):\n",
        "      if (num%i == 0):\n",
        "        suma += i\n",
        "    if suma == num:\n",
        "      return True\n",
        "    else: \n",
        "      return False\n",
        "  else:\n",
        "    return(\"El n√∫mero debe ser positivo\")\n",
        "\n"
